#include <stdio.h>

int main() {
    float a, b, c;

    printf("Enter the three sides of the triangle: ");
    scanf("%f %f %f", &a, &b, &c);

    if (a == b && b == c)
        printf("\nEquilateral Triangle\n");
    else if (a == b || b == c || a == c)
        printf("\nIsosceles Triangle\n");
    else
        printf("\nScalene Triangle\n");

    return 0;
}
