#include <stdio.h>
#include <string.h>

int main() {
    char sentence[1000];
    fgets(sentence, sizeof(sentence), stdin);

    int start = 0, i = 0;
    while(sentence[i] != '\0' && sentence[i] != '\n') {
        if(sentence[i] == ' ' || sentence[i+1] == '\0' || sentence[i+1] == '\n') {
            int end = (sentence[i] == ' ') ? i - 1 : i;
            for(int j = end; j >= start; j--)
                printf("%c", sentence[j]);
            printf(" ");
            start = i + 1;
        }
        i++;
    }

    printf("\n");
    return 0;
}
