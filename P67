#include <stdio.h>

int main() {
    int n, pos, key;
    scanf("%d", &n);

    int arr[n + 1];
    for (int i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    scanf("%d %d", &pos, &key);  // pos is 0-based index

    for (int i = n; i > pos; i--)
        arr[i] = arr[i - 1];

    arr[pos] = key;
    n++;

    for (int i = 0; i < n; i++)
        printf("%d ", arr[i]);

    return 0;
}
