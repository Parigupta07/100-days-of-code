#include <stdio.h>
struct Student{ char name[50]; int roll; float marks; };
int main(){
    struct Student s[5],maxs; for(int i=0;i<5;i++) scanf("%s %d %f",s[i].name,&s[i].roll,&s[i].marks);
    maxs=s[0];
    for(int i=1;i<5;i++) if(s[i].marks>maxs.marks) maxs=s[i];
    printf("%s %d %.2f\n",maxs.name,maxs.roll,maxs.marks);
    return 0;
}
