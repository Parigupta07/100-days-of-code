#include <stdio.h>
#include <string.h>

int main() {
    char name[1000];
    fgets(name, sizeof(name), stdin);

    int i = 0;
    if(name[i] != ' ' && name[i] != '\n')
        printf("%c", name[i]);  // Print first character

    while(name[i] != '\0' && name[i] != '\n') {
        if(name[i] == ' ' && name[i+1] != ' ' && name[i+1] != '\0' && name[i+1] != '\n')
            printf("%c", name[i+1]);  // Print first char after space
        i++;
    }

    printf("\n");
    return 0;
}
