#include <stdio.h>

int main() {
    char str[1000];
    fgets(str, sizeof(str), stdin);

    int spaces = 0, digits = 0, special = 0;
    int i = 0;

    while(str[i] != '\0' && str[i] != '\n') {
        char ch = str[i];
        if(ch == ' ')
            spaces++;
        else if(ch >= '0' && ch <= '9')
            digits++;
        else if((ch >= 'A' && ch <= 'Z') || (ch >= 'a' && ch <= 'z'))
            ; // do nothing for letters
        else
            special++;
        i++;
    }

    printf("Spaces: %d\nDigits: %d\nSpecial characters: %d\n", spaces, digits, special);

    return 0;
}
