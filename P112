#include <stdio.h>
int main(){
    int n; scanf("%d",&n); int arr[n];
    for(int i=0;i<n;i++) scanf("%d",&arr[i]);
    int maxsum=arr[0], curr=arr[0];
    for(int i=1;i<n;i++){
        curr = (curr+arr[i]>arr[i])?curr+arr[i]:arr[i];
        if(curr>maxsum) maxsum=curr;
    }
    printf("%d\n",maxsum);
    return 0;
}
