#include <stdio.h>
int main(){
    int n,k; scanf("%d %d",&n,&k); int arr[n];
    for(int i=0;i<n;i++) scanf("%d",&arr[i]);
    for(int i=0;i<=n-k;i++){
        int f=0;
        for(int j=i;j<i+k;j++){
            if(arr[j]<0){ printf("%d%s",arr[j],(i==n-k)?"\n":" "); f=1; break; }
        }
        if(!f) printf("0%s",(i==n-k)?"\n":" ");
    }
    return 0;
}
