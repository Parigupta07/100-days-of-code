#include <stdio.h>
int main(){
    int n; scanf("%d",&n); int arr[n];
    for(int i=0;i<n;i++) scanf("%d",&arr[i]);
    for(int i=0;i<n;i++){
        int pg=-1;
        for(int j=i-1;j>=0;j--){ if(arr[j]>arr[i]){ pg=arr[j]; break; } }
        printf("%d%s",pg,(i==n-1)?"\n":",");
    }
    return 0;
}
