#include <stdio.h>
int main(){
    int n; scanf("%d",&n);
    FILE *f=fopen("students.txt","w+");
    char name[50]; int roll; float marks;
    for(int i=0;i<n;i++){
        scanf("%s %d %f",name,&roll,&marks);
        fprintf(f,"%s %d %.2f\n",name,roll,marks);
    }
    rewind(f);
    while(fscanf(f,"%s %d %f",name,&roll,&marks)==3)
        printf("%s %d %.2f\n",name,roll,marks);
    fclose(f);
    return 0;
}
