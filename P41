#include <stdio.h>

int main() {
    int num, first, last, temp, digits = 0, power = 1;

    printf("Enter a number: ");
    scanf("%d", &num);

    temp = num;
    last = num % 10;

    while (temp >= 10) {
        temp /= 10;
        digits++;
        power *= 10;
    }
    first = temp;

    int middle = (num % power) / 10;
    int swapped = last * power + middle * 10 + first;

    printf("\nNumber after swapping first and last digits = %d\n", swapped);

    return 0;
}
