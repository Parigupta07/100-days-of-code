#include <stdio.h>
int main(){
    int n,target; scanf("%d",&n); int arr[n]; for(int i=0;i<n;i++) scanf("%d",&arr[i]);
    scanf("%d",&target);
    int f=0;
    for(int i=0;i<n;i++){
        for(int j=i+1;j<n;j++){
            if(arr[i]+arr[j]==target){ printf("%d %d\n",i,j); f=1; break; }
        }
        if(f) break;
    }
    if(!f) printf("-1 -1\n");
    return 0;
}
