#include <stdio.h>

int main() {
    char bin[100];
    int i;

    printf("Enter a binary number: ");
    scanf("%s", bin);

    printf("1's Complement = ");

    for (i = 0; bin[i] != '\0'; i++) {
        if (bin[i] == '0')
            printf("1");
        else if (bin[i] == '1')
            printf("0");
        else {
            printf("\nInvalid binary number\n");
            return 0;
        }
    }

    return 0;
}
