#include <stdio.h>

int main() {
    char ch;

    printf("Enter a character: ");
    scanf("%c", &ch);

    if (ch >= 'A' && ch <= 'Z')
        printf("\nUppercase Alphabet\n");
    else if (ch >= 'a' && ch <= 'z')
        printf("\nLowercase Alphabet\n");
    else if (ch >= '0' && ch <= '9')
        printf("\nDigit\n");
    else
        printf("\nSpecial Character\n");

    return 0;
}
