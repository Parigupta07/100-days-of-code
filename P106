#include <stdio.h>
int main(){
    int n; scanf("%d",&n); int arr[n];
    for(int i=0;i<n;i++) scanf("%d",&arr[i]);
    for(int i=0;i<n;i++){
        int ng=-1;
        for(int j=i+1;j<n;j++){ if(arr[j]>arr[i]){ ng=arr[j]; break; } }
        printf("%d%s",ng,(i==n-1)?"\n":",");
    }
    return 0;
}
