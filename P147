#include <stdio.h>
#include <stdlib.h>

struct Employee {
    int id;
    char name[50];
    float salary;
};

int main() {
    FILE *fp;
    struct Employee e[2] = {
        {1, "Alice", 50000},
        {2, "Bob", 60000}
    };
    struct Employee temp;

    fp = fopen("employees.dat", "wb");
    if (!fp) exit(1);
    fwrite(e, sizeof(struct Employee), 2, fp);
    fclose(fp);

    fp = fopen("employees.dat", "rb");
    if (!fp) exit(1);
    while(fread(&temp, sizeof(struct Employee), 1, fp)) {
        printf("ID: %d Name: %s Salary: %.2f\n", temp.id, temp.name, temp.salary);
    }
    fclose(fp);
    return 0;
}
