#include <stdio.h>

int main() {
    char str[1000], ch;
    fgets(str, sizeof(str), stdin);
    scanf(" %c", &ch);

    int count = 0;
    int i = 0;
    while(str[i] != '\0' && str[i] != '\n') {
        if(str[i] == ch)
            count++;
        i++;
    }

    printf("%d\n", count);

    return 0;
}
