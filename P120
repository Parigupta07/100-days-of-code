#include <stdio.h>
#include <ctype.h>
#include <string.h>
int main(){
    char s[1000]; fgets(s,sizeof(s),stdin);
    int cap=1,len=strlen(s); if(s[len-1]=='\n') s[len-1]=0;
    for(int i=0;s[i];i++){
        if(cap && s[i]>='a'&&s[i]<='z') s[i]-=32;
        else if(s[i]>='A'&&s[i]<='Z') s[i]+=32;
        cap = (s[i]=='.')?1:0;
    }
    printf("%s\n",s);
    return 0;
}
