#include <stdio.h>
#include <string.h>

int main() {
    char str[1000];
    fgets(str, sizeof(str), stdin);

    int len = strlen(str);
    if(str[len-1] == '\n') str[len-1] = '\0';
    len = strlen(str);

    for(int i = 0; i < len; i++) {
        for(int j = 1; j <= len - i; j++) {
            for(int k = i; k < i + j; k++)
                printf("%c", str[k]);
            printf("\n");
        }
    }

    return 0;
}
