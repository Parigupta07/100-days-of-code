#include <stdio.h>
#include <string.h>

int main() {
    char sentence[1000];
    fgets(sentence, sizeof(sentence), stdin);

    int maxLen = 0, len = 0, start = 0, maxStart = 0;
    for(int i = 0; sentence[i] != '\0' && sentence[i] != '\n'; i++) {
        if(sentence[i] != ' ' && sentence[i] != '\t') {
            if(len == 0) start = i; 
            len++;
        } else {
            if(len > maxLen) {
                maxLen = len;
                maxStart = start;
            }
            len = 0;
        }
    }

    if(len > maxLen) {  // Check last word
        maxLen = len;
        maxStart = start;
    }

    for(int i = maxStart; i < maxStart + maxLen; i++)
        printf("%c", sentence[i]);
    printf("\n");

    return 0;
}
