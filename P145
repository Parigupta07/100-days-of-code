#include <stdio.h>
struct Student{ char name[50]; int roll; float marks; };
struct Student top(struct Student s[], int n){
    struct Student t=s[0];
    for(int i=1;i<n;i++) if(s[i].marks>t.marks) t=s[i];
    return t;
}
int main(){
    struct Student s[5]; for(int i=0;i<5;i++) scanf("%s %d %f",s[i].name,&s[i].roll,&s[i].marks);
    struct Student t=top(s,5);
    printf("%s %d %.2f\n",t.name,t.roll,t.marks);
    return 0;
}
